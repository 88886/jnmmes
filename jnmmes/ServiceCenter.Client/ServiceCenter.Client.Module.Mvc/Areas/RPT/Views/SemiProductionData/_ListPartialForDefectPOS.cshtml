@model ServiceCenter.Client.Mvc.Areas.RPT.Models.SemiProductionViewModels
@using ServiceCenter.Client.Mvc.Resources;
@using RPTResources = ServiceCenter.Client.Mvc.Resources.RPT;
@using ServiceCenter.Client.Mvc.Areas.RPT.Models;
@using ServiceCenter.Common;
@using ServiceCenter.Model;
@using System.Data;
@{
    DataTable dtList = new System.Data.DataTable();
    dtList = ViewBag.dtList;
}
@if (dtList != null)
{
    <table class="table table-hover table-bordered table-condensed">
        <thead>
            <tr class="active">
                <td>序号</td>
                <td>批次</td>
                <td>工序</td>
                <td>线别</td>
                <td>数量</td>
                <td>日期</td>
            </tr>
        </thead>
        <tbody>
            @for (int row = 0; row < dtList.Rows.Count; row++)
            {
                <tr>
                    <td>@(row+1)</td>
                    <td>@(dtList.Rows[row]["LOT_NUMBER"]==null?"":dtList.Rows[row]["LOT_NUMBER"].ToString())</td>
                    <td>@(dtList.Rows[row]["LINE_CODE"]==null?"":dtList.Rows[row]["LINE_CODE"].ToString())</td>
                    <td>@(dtList.Rows[row]["ROUTE_STEP_NAME"]==null?"":dtList.Rows[row]["ROUTE_STEP_NAME"].ToString())</td>
                    <td>@(dtList.Rows[row]["qty"]==null?"":dtList.Rows[row]["qty"].ToString())</td>
                    <td>@(dtList.Rows[row]["qty"]==null?"":dtList.Rows[row]["EDIT_TIME"].ToString())</td>

                </tr>
            }
        </tbody>
    </table>
}
