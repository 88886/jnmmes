@model ServiceCenter.Client.Mvc.Areas.FMM.Models.ClientConfigAttributeViewModel
@using ServiceCenter.Common;
@using ServiceCenter.Client.Mvc.Resources;
@Html.ValidationSummary(true)


<div class="form-group">
    @Html.LabelFor(m => m.ItemNo, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.ItemNo</p>
        @Html.HiddenFor(m => m.ItemNo)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.ClientName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.ClientName</p>
        @Html.HiddenFor(m => m.ClientName)
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(m => m.AttributeName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.AttributeName</p>
        @Html.HiddenFor(m => m.AttributeName)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.Value, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Value, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.Value)
    </div>
</div>
