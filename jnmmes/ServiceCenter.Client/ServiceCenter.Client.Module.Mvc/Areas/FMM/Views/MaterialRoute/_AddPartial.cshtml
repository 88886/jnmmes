@model ServiceCenter.Client.Mvc.Areas.FMM.Models.MaterialRouteViewModel
@using ServiceCenter.MES.Model.FMM;
@{
    //MaterialType obj = ViewBag.MaterialType;
}
<div class="form-group">
    @Html.LabelFor(m => m.MaterialCode, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.MaterialCode</p>
        @Html.HiddenFor(m => m.MaterialCode)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.LocationName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.DropDownListFor(m => Model.LocationName
                              , Model.GetLocationNameList()
                              , new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.LocationName)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.RouteEnterpriseName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.DropDownListFor(m => Model.RouteEnterpriseName
                              , Model.GetRouteEnterpriseNameList()
                              , new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.RouteEnterpriseName)
    </div>
</div>
