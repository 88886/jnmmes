@model ServiceCenter.Client.Mvc.Areas.FMM.Models.MaterialChestParameterViewModel
@using ServiceCenter.Common;
@using ServiceCenter.Client.Mvc.Resources;
@Html.ValidationSummary(true)

<div class="form-group">
    @* 产品编码 *@
    @Html.LabelFor(m => m.ProductCode, new { @class = "col-md-5 control-label" })
    <div class="col-md-7">
        <p class="form-control-static">@Model.ProductCode</p>
    </div>
</div>
<div class="form-group">
    @* 颜色是否不可混 *@
    @Html.LabelFor(m => m.ColorLimit, new { @class = "col-md-5 control-label" })
    <div class="col-md-7">
        <p class="form-control-static">@(Model.ColorLimit ? StringResource.Yes : StringResource.No)</p>
    </div>
</div>
