@model ServiceCenter.Client.Mvc.Areas.FMM.Models.CalibrationPlateViewModel

@Html.ValidationSummary(true)

<div class="form-group">
    @Html.LabelFor(m => m.CalibrationPlateID, new { @class = "col-md-3 control-label" })
