        @Html.DropDownListFor(m => m.CalibrationPlateType, @Model.GetTypeList(), new { @class = "form-control" })
        @*@Html.TextBoxFor(m => m.CalibrationPlateType, new { @class = "form-control" })*@
        @Html.ValidationMessageFor(m => m.CalibrationPlateType)
    </div>
</div>
