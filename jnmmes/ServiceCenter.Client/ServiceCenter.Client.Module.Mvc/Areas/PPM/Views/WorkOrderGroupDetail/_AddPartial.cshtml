@model ServiceCenter.Client.Mvc.Areas.PPM.Models.WorkOrderGroupDetailViewModel
@*@{
    var GroupNoList = (IEnumerable<SelectListItem>)ViewBag.GroupNoList;
}*@
@using ServiceCenter.Common;
@Html.ValidationSummary(true)
    <div class="form-group">
        @Html.LabelFor(m => m.WorkOrderGroupNo, new { @class = "col-md-3 control-label" })
        <div class="col-md-9">
            @Html.DropDownListFor(m => m.WorkOrderGroupNo, @Model.GetWorkOrderGroupNoList(), new { @class = "form-control", style = "text-transform:uppercase;ime-mode:inactive" })
            @Html.ValidationMessageFor(m => m.WorkOrderGroupNo)
        </div>
    </div>
    <div class="form-group" >
        @Html.LabelFor(m => m.OrderNumber, new { @class = "col-md-3 control-label" })
        <div class="col-md-9">
            @Html.TextBoxFor(m => m.OrderNumber, new { @id = "txtOrderNumber1",@class = "form-control", style = "text-transform:uppercase;ime-mode:inactive" })
            @Html.ValidationMessageFor(m => m.OrderNumber)
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.ProductCode, new { @class = "col-md-3 control-label" })
        <div class="col-md-9">
            @Html.TextBoxFor(m => m.ProductCode, new { @id = "txtProductCode1", @class = "form-control", style = "text-transform:uppercase;ime-mode:inactive", @readonly = "readonly" })
            @Html.ValidationMessageFor(m => m.ProductCode)
        </div>
    </div>
    

