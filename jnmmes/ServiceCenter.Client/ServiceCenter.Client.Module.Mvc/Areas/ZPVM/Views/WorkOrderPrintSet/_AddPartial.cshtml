@model ServiceCenter.Client.Mvc.Areas.ZPVM.Models.WorkOrderPrintSetViewModel
<div class="form-group">
    @Html.LabelFor(m => m.OrderNumber, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.OrderNumber</p>
        @Html.HiddenFor(m => m.OrderNumber)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.MaterialCode, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.MaterialCode</p>
        @Html.HiddenFor(m => m.MaterialCode)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.LabelCode, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.DropDownListFor(m => m.LabelCode, @Model.GetLabelCodeList(), new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.LabelCode)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.ItemNo, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
      @Html.TextBoxFor(m => m.ItemNo, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.ItemNo)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.Qty, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Qty, new { @class = "form-control", @id = "txtAddDecayCode" })
        @Html.ValidationMessageFor(m => m.Qty)
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(m => m.KeyCode, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.KeyCode, new { @class = "form-control", @id = "txtAddCode", style = "text-transform:uppercase;ime-mode:inactive" })
        @Html.ValidationMessageFor(m => m.KeyCode)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.Code, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.Code, new { @class = "form-control", @readonly = "readonly" })
        @Html.ValidationMessageFor(m => m.Code)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.PowerItemNo, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.PowerItemNo, new { @class = "form-control", @readonly = "readonly" })
        @Html.ValidationMessageFor(m => m.PowerItemNo)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.PowerName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.TextBoxFor(m => m.PowerName, new { @class = "form-control", @readonly = "readonly" })
        @Html.ValidationMessageFor(m => m.PowerName)
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(m => m.IsUsed, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.CheckBoxFor(m => m.IsUsed, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.IsUsed)
    </div>
</div>
