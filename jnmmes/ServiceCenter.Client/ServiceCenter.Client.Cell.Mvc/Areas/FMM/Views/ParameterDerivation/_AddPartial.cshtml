@model ServiceCenter.Client.Mvc.Areas.FMM.Models.ParameterDerivationViewModel
<div class="form-group">
    @Html.LabelFor(m => m.DerivedParameterName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        <p class="form-control-static">@Model.DerivedParameterName</p>
        @Html.HiddenFor(m => m.DerivedParameterName)
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.ParameterName, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.DropDownListFor(m => Model.ParameterName
                                , Model.GetParameterName(Model.DerivedParameterName)
                                , new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.ParameterName)
    </div>
</div>