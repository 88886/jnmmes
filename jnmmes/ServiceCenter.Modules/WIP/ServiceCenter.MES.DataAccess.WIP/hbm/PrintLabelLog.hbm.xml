    <property name="ItemNo" column="ITEM_NO"/>
    <property name="PrintTime" column="PRINT_TIME"/>
    <property name="EditTime" column="EDIT_TIME"/>
    <property name="TrackFlag" column="TRACK_FLAG"/>
  </class>
</hibernate-mapping>
