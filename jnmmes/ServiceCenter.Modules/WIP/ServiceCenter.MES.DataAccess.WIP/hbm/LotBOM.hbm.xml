<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.WIP" 
                   assembly="ServiceCenter.MES.Model.WIP">
  <class name="LotBOM" table="WIP_LOT_BOM">
