<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.FMM" 
                   assembly="ServiceCenter.MES.Model.FMM">
  <class name="PrintLabel" table="FMM_PRINTLABEL">
    <id name="Key" column="LABEL_CODE">
      <generator class="assigned" />
    </id>
    <property name="Name" column="LABEL_NAME"/>
    <property name="Content" column="LABEL_CONTENT"/>
    <property name="Type" column="LABEL_TYPE"/>
    <property name="IsUsed" column="IS_USED"/>
    <property name="Creator" column="CREATOR"/>
    <property name="CreateTime" column="CREATE_TIME"/>
    <property name="Editor" column="EDITOR"/>
    <property name="EditTime" column="EDIT_TIME"/>
  </class>
</hibernate-mapping>