<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.FMM" 
                   assembly="ServiceCenter.MES.Model.FMM">
  <class name="LineStore" table="FMM_LINE_STORE">
    <id name="Key" column="STORE_NAME">
      <generator class="assigned" />
    </id>
    <property name="Type" column="STORE_TYPE"/>
    <property name="RequestFlag" column="REQUEST_FLAG"/>
    <property name="Status" column="OBJECT_STATUS"/>
    <property name="LocationName" column="LOCATION_NAME"/>
    <property name="RouteOperationName" column="ROUTE_OPERATION_NAME"/>
    <property name="Description" column="DESCRIPTION"/>
    <property name="Creator" column="CREATOR"/>
    <property name="CreateTime" column="CREATE_TIME"/>
    <property name="Editor" column="EDITOR"/>
    <property name="EditTime" column="EDIT_TIME"/>
  </class>
</hibernate-mapping>