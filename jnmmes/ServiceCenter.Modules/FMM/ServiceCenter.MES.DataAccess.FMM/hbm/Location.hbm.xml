<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.FMM" 
                   assembly="ServiceCenter.MES.Model.FMM">
  <class name="Location" table="FMM_LOCATION">
    <id name="Key" column="LOCATION_NAME">
      <generator class="assigned" />
    </id>
    <property name="Description" column="DESCRIPTION"/>
    <property name="Level" column="LOCATION_LEVEL"/>
    <property name="ParentLocationName" column="PARENT_LOCATION_NAME"/>
    <property name="ERPDeptCode" column="ERPDeptCode"/>
    <property name="Creator" column="CREATOR"/>
    <property name="CreateTime" column="CREATE_TIME"/>
    <property name="Editor" column="EDITOR"/>
    <property name="EditTime" column="EDIT_TIME"/>
  </class>
</hibernate-mapping>