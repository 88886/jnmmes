<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.FMM" 
                   assembly="ServiceCenter.MES.Model.FMM">
  <class name="EquipmentLayout" table="FMM_EQUIPMENT_LAYOUT">
    <id name="Key" column="LAYOUT_NAME">
      <generator class="assigned" />
    </id>
    <property name="Description" column="LAYOUT_DESC"/>
    <property name="Width" column="WIDTH"/>
    <property name="Height" column="HEIGHT"/>
    <property name="BackgroundImage" column="LAYOUT_PIC" type="BinaryBlob"/>
    <property name="Status" column="STATUS"/>
    <property name="Creator" column="CREATOR"/>
    <property name="CreateTime" column="CREATE_TIME"/>
    <property name="Editor" column="EDITOR"/>
    <property name="EditTime" column="EDIT_TIME"/>
  </class>
</hibernate-mapping>