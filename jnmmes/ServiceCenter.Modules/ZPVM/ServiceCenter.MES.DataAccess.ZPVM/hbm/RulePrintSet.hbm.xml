<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ServiceCenter.MES.Model.ZPVM" 
                   assembly="ServiceCenter.MES.Model.ZPVM">
  <class name="RulePrintSet" table="ZFMM_RULE_PRINTSET">
    <composite-id  name="Key" class="RulePrintSetKey">
      <key-property name="Code" column="RULE_CODE"/>
      <key-property name="LabelCode" column="LABEL_CODE"/>
    </composite-id >
    <property name="ItemNo" column="ITEM_NO"/>
    <property name="Qty" column="PRINT_QTY"/>
    <property name="IsUsed" column="IS_USED"/>
    <property name="Creator" column="CREATOR"/>
    <property name="CreateTime" column="CREATE_TIME"/>
    <property name="Editor" column="EDITOR"/>
    <property name="EditTime" column="EDIT_TIME"/>
  </class>
</hibernate-mapping>